{
    "name": "Gravatar Redirect",
    "description": "Redirect gravatar source request to mirror",
    "version": "0.0.2",
    "manifest_version": 3,
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestFeedback"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "redirect",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    },
    "icons": {
        "512": "icon.png"
    },
    "author": "nondanee"
}